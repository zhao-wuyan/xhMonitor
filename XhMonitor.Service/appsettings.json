{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "XhMonitor": "Debug"
    }
  },
  "ConnectionStrings": {
    "DatabaseConnection": "Data Source=xhmonitor.db;Mode=ReadWriteCreate;Cache=Shared"
  },
  "Monitor": {
    "IntervalSeconds": 5,
    "Keywords": ["--port 8188","llama-server"]
  },
  "MetricProviders": {
    "PluginDirectory": ""
  },
  "Server": {
    "Host": "localhost",
    "Port": 35179,
    "HubPath": "/hubs/metrics"
  }
}
